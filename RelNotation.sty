\def\rel{\leftrightarrow}
\def\RELcomp{\kern-.03ex\raise.4ex\hbox{\tiny\bf;}\kern0.05ex}
\def\RELroundSymbols{
\def\RELleq{\mathrel{\subseteq}}
\def\RELlt{\mathrel{\subset}}
\def\RELgeq{\mathrel{\supseteq}}
\def\RELgt{\mathrel{\supset}}
\def\RELjoin{\mathbin{\cup}}
\def\RELmeet{\mathbin{\cap}}
\def\RELjoinSet{\mathop{\bigcup}}
\def\RELmeetSet{\mathop{\bigcap}}
}
\def\bigsqcap{\lower.5ex\hbox{\LARGE${\sqcap}$}}
\def\bigsqcup{\lower.5ex\hbox{\LARGE${\sqcup}$}}
\def\RELsquareSymbols{
\def\RELleq{\mathrel{\sqsubseteq}}
\def\RELlt{\mathrel{\sqsubset}}
\def\RELgeq{\mathrel{\sqsupseteq}}
\def\RELgt{\mathrel{\sqsupset}}
\def\RELjoin{\mathbin{\sqcup}}
\def\RELmeet{\mathbin{\sqcap}}
\def\RELjoinSet{\mathop{\bigsqcup}}
\def\RELmeetSet{\mathop{\bigsqcap}}
}
\RELroundSymbols
\def\SETleq{\mathrel{\subseteq}}
